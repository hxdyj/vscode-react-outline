{
  "name": "bbl-react-outline",
  "displayName": "BBL React Outline",
  "description": "A simple extension that shows the symbols and outlines for a React file. Fork of https://github.com/ShubhamVerma1811/vscode-react-outline",
  "version": "0.0.1",
  "publisher": "hxdyj",
  "preview": true,
  "icon": "public/assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/hxdyj/vscode-react-outline.git"
  },
  "homepage": "https://github.com/hxdyj/vscode-react-outline",
  "engines": {
    "vscode": "^1.62.0"
  },
  "capabilities": {
    "documentSymbolProvider": "true"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.5",
    "@types/eslint": "^8.56.5",
    "@types/estree": "^1.0.5",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "glob": "^7.1.7",
    "prettier": "^2.5.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0"
  }
}
